    "clean": "rm -rf ./lib/*",
    "lint": "eslint 'src/*.js' 'src/**/*.js'; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akute-health-inc/draft-js-macro-plugin.git"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.0.4",
    "immutable": "~3.7.4",
    "lodash": "^4.17.14",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1 || ^0.11.0",
    "react": "^16.3.0"
  }
}
