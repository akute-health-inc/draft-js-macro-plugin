{
  "name": "draft-js-macro-plugin",
  "version": "1.0.0",
  "description": "Macro Plugin for DraftJS",
  "author": {
    "name": "Sharud Agarwal",
    "email": "info@akutehealth.com",
    "url": "https://www.akutehealth.com"
  },
  "main": "lib/index.cjs.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "compile": "npm run build",
    "precompile": "npm run clean",
    "prepublish": "npm run compile",
    "test": "NODE_ENV=test babel-node test | faucet",
    "posttest": "npm run lint",
    "clean": "rm -rf ./lib/*",
    "lint": "eslint 'src/*.js' 'src/**/*.js'; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akute-health-inc/draft-js-macro-plugin.git"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.0.4",
    "immutable": "~3.7.4",
    "lodash": "^4.17.14",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1 || ^0.11.0",
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-cli": "6.7.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "faucet": "0.0.1",
    "is-function": "1.0.1",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tape": "4.5.1"
  }
}
