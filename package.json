{
  "name": "draft-js-macro-plugin",
  "version": "1.0.0",
  "description": "Macro Plugin for DraftJS",
  "author": {
    "name": "Sharud Agarwal",
    "email": "info@akutehealth.com",
    "url": "https://www.akutehealth.com"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "compile": "npm run build",
    "precompile": "npm run clean",
    "prepublish": "npm run compile",
    "test": "NODE_ENV=test babel-node test | faucet",
